n    alt=\"img\"\r\n/>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </section>\r\n   \r\n  </template>\r\n  \r\n  <script setup>\r\n\r\n  </script>\r\n <style scoped>\r\n .first-section {\r\n    background-color: #FFFBF0;\r\n  }\r\n  .cv-btn {\r\n    display: inline-flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    padding: 0.75rem 1.5rem;\r\n    border: none;\r\n    border-radius: 12px;\r\n  \r\n    font-size: 1rem;\r\n    font-weight: 500;\r\n    background-color: #ff5c00;\r\n    color: white;\r\n    cursor: pointer;\r\n    transition: background-color 0.3s ease, transform 0.2s ease;\r\n    text-align: center;\r\n    text-decoration: none;\r\n    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\r\n  }\r\n  .cv-btn:hover{\r\n    background-color: #7b4c2e;\r\n\r\n  }\r\n </style>","export default \"__VITE_ASSET__B5pw6UUD__\"","export default \"__VITE_ASSET__DDxsjPgp__\"","export default \"__VITE_ASSET__y4nGuUAo__\"","export default \"__VITE_ASSET__Cke595yL__\"","export default \"__VITE_ASSET__BUkKDK0i__\"","export default \"__VITE_ASSET__B3k9VSOs__\"","export default \"__VITE_ASSET__QBZxMnkK__\"","export default \"__VITE_ASSET__BzWouZ9b__\"","<template>\r\n    <section class=\"skills-section container\">\r\n      <h2 class=\"section-title\">Vaardigheden</h2>\r\n  \r\n      <div class=\"skills-grid\">\r\n        <div class=\"skill-card\" v-for=\"skill in skills\" :key=\"skill.title\">\r\n          <div class=\"skill-image\">\r\n  <img :src=\"skill.image\" :alt=\"skill.title\" loading=\"lazy\"/>\r\n</div>\r\n          <h3>{{ skill.title }}</h3>\r\n          <p>{{ skill.description }}</p>\r\n          <ul>\r\n            <li v-for=\"item in skill.items\" :key=\"item\">{{ item }}</li>\r\n          </ul>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </template>\r\n  \r\n  <script setup>\r\n  import webdesign from '@/assets/img/webdesign.webp'\r\nimport webdevelopment from '@/assets/img/webdevelopment.webp'\r\nimport wordpress from '@/assets/img/wordpress.webp'\r\nimport db from '@/assets/img/db.webp'\r\nimport uxui from '@/assets/img/ux-ui.webp'\r\nimport designvideo from '@/assets/img/design-and-videos.webp'\r\nimport mobileai from '@/assets/img/mobile-and-ai.webp'\r\nimport coding from '@/assets/img/coding.webp'\r\n  const skills = [\r\n  {\r\n    title: 'Webdesign',\r\n    description: 'Moderne front-end met frameworks en CSS tools.',\r\n    image: webdesign,\r\n    items: ['Nuxt', 'Next', 'Vue', 'React', 'jQuery', 'SCSS', 'TailwindCSS', 'UnoCSS'],\r\n  },\r\n  {\r\n    title: 'Webontwikkeling (Back-End)',\r\n    description: 'API’s en servers met moderne tools.',\r\n    image: webdevelopment,\r\n    items: ['Express.js', 'Fastify', 'JWT', 'Passport.js', 'Multer', 'Socket.io', 'Nodemailer', 'dotenv', 'CORS', 'Cron Jobs'],\r\n  },\r\n  {\r\n    title: 'Custom Front & WordPress',\r\n    description: 'Interactieve front-end, GSAP animaties en WordPress plugins.',\r\n    image: wordpress,\r\n    items: ['Axios','Bootstrap', 'Vuetify','I18n', 'GSAP', 'Lenis', 'Vite', 'Matters.js', 'Tree.js', 'WordPress', 'WooCommerce', 'Elementor', 'ACF', 'WPML', 'Yoast SEO', 'WordPress REST API'],\r\n  },\r\n  {\r\n    title: 'Databases',\r\n    description: 'Opslag en beheer van data.',\r\n    image: db,\r\n    items: ['MongoDB', 'Mongoose', 'Directus', 'Prisma'],\r\n  },\r\n  {\r\n    title: 'UI / UX Design',\r\n    description: 'Visueel ontwerp en gebruikerservaring.',\r\n    image: uxui,\r\n    items: ['Figma', 'Adobe XD'],\r\n  },\r\n  {\r\n    title: 'Grafisch & Video',\r\n    description: 'Creatie van visuals en video content.',\r\n    image: designvideo,\r\n    items: ['Photoshop', 'Illustrator', 'InDesign', 'Premiere', 'After Effects'],\r\n  },\r\n  {\r\n    title: 'Mobiel & AI (in progress)',\r\n    description: 'Huidige focus op nieuwe technologieën.',\r\n    image: mobileai,\r\n    items: ['Flutter', 'Dart', 'Python', 'FastAPI'],\r\n  },\r\n  {\r\n    title: 'Talen & Tools',\r\n    description: 'Continue ontwikkeling in diverse talen.',\r\n    image: coding,\r\n    items: ['JavaScript (goed)', 'PHP / TS / Dart (gemiddeld)', 'Python / C++ (basis)'],\r\n  },\r\n]\r\n\r\n\r\n  </script>\r\n  ","<template>\r\n    <section class=\"tools-section\">\r\n      <div class=\"section-header container\">\r\n        <h2>Workflow & Tools</h2>\r\n        <p>\r\n          Ik maak gebruik van moderne tools en development workflows om projecten efficiënt,\r\n          schaalbaar en gestructureerd te houden. Van versiebeheer tot automatisering,\r\n          ik ben vertrouwd met tools die het verschil maken in professionele omgevingen.\r\n        </p>\r\n      </div>\r\n  \r\n      <div class=\"marquee\">\r\n        <div class=\"marquee-track\">\r\n          <div class=\"tool-name\" v-for=\"tool in repeatedTools\" :key=\"tool + Math.random()\">\r\n            {{ tool }}\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </template>\r\n  \r\n  <script setup>\r\n   const tools = [\r\n  // Frontend frameworks & tools\r\n  'Nuxt', 'Next', 'Vue', 'React', 'jQuery', 'SCSS', 'TailwindCSS', 'UnoCSS', 'Bootstrap', 'Vuetify', 'I18n',\r\n\r\n  // Backend & API\r\n  'Express.js', 'Fastify', 'JWT', 'Passport.js', 'Multer', 'Socket.io', 'Nodemailer', 'dotenv', 'CORS', 'Cron Jobs',\r\n\r\n  // WordPress & custom\r\n  'GSAP', 'Lenis', 'Vite', 'Matters.js', 'Tree.js', 'Axios', 'WordPress', 'WooCommerce', 'Elementor', 'ACF', 'WPML', 'Yoast SEO',\r\n\r\n  // Databases\r\n  'MongoDB', 'Mongoose', 'Directus', 'Redis',\r\n\r\n  // UI/UX & design\r\n  'Figma', 'Adobe XD',\r\n\r\n  // Graphic & video\r\n  'Photoshop', 'Illustrator', 'InDesign', 'Premiere', 'After Effects',\r\n\r\n  // Mobile & AI\r\n  'Flutter', 'Dart', 'Python', 'FastAPI',\r\n\r\n  // General languages & tools\r\n  'JavaScript', 'TypeScript', 'PHP', 'C++', 'Git', 'GitHub', 'Docker', 'Pug.js', 'Gulp.js', 'WSL', 'Swagger', 'Prettier', 'ESLint', 'Trello', 'Notion', 'Postman', 'Cloudflare'\r\n]\r\nconst repeatedTools = [...tools, ...tools, ...tools]\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .tools-section {\r\n    min-height: 50vh;\r\n    background-color: #FFFBF0;\r\ndisplay: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    padding: 4rem 0;\r\n    overflow: hidden;\r\n  }\r\n  \r\n  .section-header {\r\n    text-align: center;\r\n    max-width: 700px;\r\n    margin: 0 auto 2rem;\r\n  }\r\n  \r\n  .marquee {\r\n    position: relative;\r\n    overflow: hidden;\r\n    width: 100%;\r\n  }\r\n  \r\n  .marquee-track {\r\n    display: flex;\r\n    width: fit-content;\r\n    animation: scroll-left 90s linear infinite;\r\n  }\r\n  \r\n  .tool-name {\r\n    flex: 0 0 auto;\r\n    padding: 0 2rem;\r\n    font-size: 1rem;\r\n    font-weight: 500;\r\n    color: #2e2e2e;\r\n    white-space: nowrap;\r\n  }\r\n  \r\n  @keyframes scroll-left {\r\n    0% {\r\n      transform: translateX(0%);\r\n    }\r\n    100% {\r\n      transform: translateX(-33.333%);\r\n    }\r\n  }\r\n  </style>\r\n  ","<template>\r\n  <section class=\"contact-section container\">\r\n    <div class=\"contact-grid\">\r\n      <div class=\"contact-info\">\r\n        <div class=\"contact-info-inner\">\r\n          <h3>Volg mij</h3>\r\n          <p>\r\n            Wil je samenwerken aan een creatief project, heb je een interessant idee, of gewoon zin in een gesprek over design en technologie?<br />\r\n            Laat van je horen via één van de onderstaande platforms.\r\n          </p>\r\n          <div class=\"social-links\">\r\n            <a href=\"https://github.com/Ali-Al-Hasan\" target=\"_blank\" rel=\"noopener\">GitHub</a>\r\n            <a href=\"https://www.behance.net/ZiouxV\" target=\"_blank\" rel=\"noopener\">Behance</a>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"contact-form-wrapper\">\r\n        <h2>Neem contact op</h2>\r\n        <p>Heb je een vraag, idee of project? Vul het formulier in en je bericht wordt rechtstreeks naar ons e-mailadres gestuurd.</p>\r\n        <form class=\"contact-form\" action=\"https://formsubmit.co/techubhq@gmail.com\" method=\"POST\">\r\n          <input type=\"text\" name=\"name\" placeholder=\"Naam\" required />\r\n          <input type=\"email\" name=\"email\" placeholder=\"E-mail\" required />\r\n          <textarea name=\"message\" placeholder=\"Bericht\" rows=\"5\" required></textarea>\r\n          <button type=\"submit\">Versturen</button>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </section>\r\n</template>\r\n\r\n<script setup>\r\n</script>\r\n\r\n<style scoped>\r\n.contact-section {\r\n  padding: 4rem 1rem;\r\n}\r\n\r\n.contact-grid {\r\n  display: flex;\r\n  flex-direction: row;\r\n  gap: 2rem;\r\n  justify-content: space-between;\r\n  align-items: stretch;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.contact-info,\r\n.contact-form-wrapper {\r\n  flex: 1 1 100%;\r\n  min-width: 300px;\r\n}\r\n\r\n@media (min-width: 768px) {\r\n  .contact-info,\r\n  .contact-form-wrapper {\r\n    flex: 1 1 45%;\r\n  }\r\n}\r\n\r\n.contact-info {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  padding: 2rem;\r\n}\r\n\r\n.contact-info-inner {\r\n  max-width: 500px;\r\n  text-align: center;\r\n}\r\n\r\n.contact-info h3 {\r\n  font-size: 1.6rem;\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n.contact-info p {\r\n  font-size: 1rem;\r\n  line-height: 1.6;\r\n  margin-bottom: 1.5rem;\r\n}\r\n\r\n.social-links {\r\n  margin-top: 1rem;\r\n  display: flex;\r\n  flex-direction: row;\r\n  gap: 0.75rem;\r\n  justify-content: center;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.social-links a {\r\n  color: #ff5c00;\r\n  text-decoration: none;\r\n  font-weight: bold;\r\n}\r\n\r\n.social-links a:hover {\r\n  text-decoration: underline;\r\n}\r\n\r\n.contact-form-wrapper {\r\n  padding: 2rem;\r\n  border-radius: 16px;\r\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.05);\r\n  background: #fff;\r\n}\r\n\r\n.contact-form {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 1rem;\r\n}\r\n\r\n.contact-form input,\r\n.contact-form textarea {\r\n  padding: 1rem;\r\n  border: 1px solid #ccc;\r\n  border-radius: 12px;\r\n  font-size: 1rem;\r\n  width: 100%;\r\n  background-color: #fff;\r\n  transition: border-color 0.2s ease;\r\n}\r\n\r\n.contact-form input:focus,\r\n.contact-form textarea:focus {\r\n  outline: none;\r\n  border-color: #ff5c00;\r\n}\r\n\r\n.contact-form button {\r\n  padding: 1rem;\r\n  background-color: #ff5c00;\r\n  color: #fff;\r\n  font-weight: bold;\r\n  border: none;\r\n  border-radius: 12px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.contact-form button:hover {\r\n  background-color: #7b4c2e;\r\n}\r\n</style>\r\n","<template>\r\n    <div>\r\n        <HeroSection />\r\n        <SkillsSection />\r\n        <WorkflowTools />\r\n       <!--  <ProjectsCanvas /> -->\r\n        <Contact />\r\n    </div>\r\n</template>"],"version":3}